package com.cafe.dao;

import java.util.List;

import com.cafe.vo.Seat;


public interface SeatDao {
	//根据用户查询条件，进行分页查询
	public List<Seat> findAllUser(String sid,int currentPage,int lineSize);
	//更新座位信息,orgUserId原始用户id   update demo_user set userid = ?[02] where userid = ?[01]
	public void updateSeat(String orgsid);
	//根据查询条件，返回用户的记录数 count
	public int getSeatCount(String state);
	public List<Seat> findAllUser1();
	public void updateSeat1(String orgsid);

}
